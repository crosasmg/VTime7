	Public Function FindDebtCli(ByVal sCertype As String, ByVal nBranch As Integer, ByVal nProduct As Integer, ByVal nPolicy As Double, ByVal nCertif As Double, ByVal nRole As Integer, ByVal dEffecdate As Date) As Boolean
		Dim lrecRoles As eRemoteDB.Execute
		
		On Error GoTo FindDebtCli_Err
		
		'+Definición de parámetros para stored procedure 'rearrCreditSales_tmp'
		lrecRoles = New eRemoteDB.Execute
