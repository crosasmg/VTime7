	'**% Add: Adds a new instance of the Tab_damage class to the collection
	'% Add: Añade una nueva instancia de la clase Tab_damage a la colección
	Public Function Add(ByVal nBranch As Integer, ByVal nDamage_cod As Integer, ByVal sDescript As String, ByVal sShort_des As String, ByVal sStatregt As String) As Tab_damage
		'create a new object
		Dim objNewMember As Tab_damage
		objNewMember = New Tab_damage
		
		With objNewMember
			.nBranch = nBranch
			.nDamage_cod = nDamage_cod
			.sDescript = sDescript
			.sShort_des = sShort_des
			.sStatregt = sStatregt
		End With
		
		'set the properties passed into the method
		mCol.Add(objNewMember)
		
		'return the object created
		Add = objNewMember
		'UPGRADE_NOTE: Object objNewMember may not be destroyed until it is garbage collected. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6E35BFF6-CD74-4B09-9689-3E1A43DF8969"'
		objNewMember = Nothing
		
	End Function
	
	'**% Find: Restores a collection of objects of Tab_damage type
	'% Find: Devuelve una coleccion de objetos de tipo Tab_damage
	'------------------------------------------------------------
	Public Function Find(ByVal nBranch As Integer) As Boolean
		'------------------------------------------------------------
		
		'**- Variable definition lrecTab_damage that will be used as a cursor
		'- Se define la variable lrecTab_damage que se utilizará como cursor.
		Dim lrecTab_damage As eRemoteDB.Execute
		
		lrecTab_damage = New eRemoteDB.Execute
		
		'**+ Execute the store procedure that searches the damages
		'+ Se ejecuta el store procedure que busca los daños
		
		With lrecTab_damage
			.StoredProcedure = "reaTab_damage"
			.Parameters.Add("nBranch", nBranch, eRemoteDB.Parameter.eRmtDataDir.rdbParamInput, eRemoteDB.Parameter.eRmtDataType.rdbInteger, 22, 0, 10, eRemoteDB.Parameter.eRmtDataAttrib.rdbParamNullable)
			'UPGRADE_WARNING: Use of Null/IsNull() detected. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="2EED02CB-5C0E-4DC1-AE94-4FAA3A30F51A"'
			.Parameters.Add("nDamage_cod", System.DBNull.Value, eRemoteDB.Parameter.eRmtDataDir.rdbParamInput, eRemoteDB.Parameter.eRmtDataType.rdbInteger, 22, 0, 10, eRemoteDB.Parameter.eRmtDataAttrib.rdbParamNullable)
			
			If Not .Run Then
				Find = False
			Else
				Find = True
				Do While Not .EOF
					Call Add(nBranch, .FieldToClass("nDamage_cod"), .FieldToClass("sDescript"), .FieldToClass("sShort_des"), .FieldToClass("sStatregt"))
					.RNext()
				Loop 
			End If
		End With
		
		'UPGRADE_NOTE: Object lrecTab_damage may not be destroyed until it is garbage collected. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6E35BFF6-CD74-4B09-9689-3E1A43DF8969"'
		lrecTab_damage = Nothing
	End Function
	
