	'%Find: Lee los datos de la tabla
	Public Function Find(ByVal nBranch As Integer, ByVal nProduct As Integer, ByVal dEffecdate As Date, Optional ByVal lblnFind As Boolean = False) As Boolean
		Dim lrecReaProduct_limits As eRemoteDB.Execute
		Dim lclsProduct_limits As Product_limits
		
		On Error GoTo Find_Err
		Find = True
		
		lrecReaProduct_limits = New eRemoteDB.Execute
		
		'+Definición de parámetros para stored procedure 'ReaProduct_limits_a'
		With lrecReaProduct_limits
			.StoredProcedure = "product_limits_sqlpkg.rea_cur_prod"
			.Parameters.Add("p_nBranch", nBranch, eRemoteDB.Parameter.eRmtDataDir.rdbParamInput, eRemoteDB.Parameter.eRmtDataType.rdbInteger, 22, 0, 10, eRemoteDB.Parameter.eRmtDataAttrib.rdbParamNullable)
			.Parameters.Add("p_nProduct", nProduct, eRemoteDB.Parameter.eRmtDataDir.rdbParamInput, eRemoteDB.Parameter.eRmtDataType.rdbInteger, 22, 0, 10, eRemoteDB.Parameter.eRmtDataAttrib.rdbParamNullable)
			.Parameters.Add("dEffecdate", dEffecdate, eRemoteDB.Parameter.eRmtDataDir.rdbParamInput, eRemoteDB.Parameter.eRmtDataType.rdbDBTimeStamp, 0, 0, 0, eRemoteDB.Parameter.eRmtDataAttrib.rdbParamNullable)
			If .Run Then
				Find = True
				Do While Not .EOF
					lclsProduct_limits = New Product_limits
					lclsProduct_limits.nBranch = .FieldToClass("nBranch")
					lclsProduct_limits.nProduct = .FieldToClass("nProduct")
					lclsProduct_limits.nLimit_type = .FieldToClass("nLimit_type")
					lclsProduct_limits.nLimit_code = .FieldToClass("nLimit_code")
					lclsProduct_limits.nValmax = .FieldToClass("nValmax")
					lclsProduct_limits.nValmin = .FieldToClass("nValmin")
					lclsProduct_limits.dCompdate = .FieldToClass("dCompdate")
					lclsProduct_limits.nUsercode = .FieldToClass("nUsercode")
					
					Call Add(lclsProduct_limits)
					'UPGRADE_NOTE: Object lclsProduct_limits may not be destroyed until it is garbage collected. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6E35BFF6-CD74-4B09-9689-3E1A43DF8969"'
					lclsProduct_limits = Nothing
					.RNext()
				Loop 
				.RCloseRec()
			Else
				Find = False
			End If
		End With
		
Find_Err: 
		If Err.Number Then
			Find = False
		End If
		'UPGRADE_NOTE: Object lrecReaProduct_limits may not be destroyed until it is garbage collected. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6E35BFF6-CD74-4B09-9689-3E1A43DF8969"'
		lrecReaProduct_limits = Nothing
		On Error GoTo 0
	End Function
	
