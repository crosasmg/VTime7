	'%Add: Agrega un nuevo registro a la colección
	Public Function Add(ByVal objClass As Product_limits) As Product_limits
		If objClass Is Nothing Then
			objClass = New Product_limits
		End If
		
		With objClass
			mCol.Add(objClass, "CP" & .nBranch & .nProduct & .nLimit_type & .nLimit_code)
		End With
		
		'Return the object created
		Add = objClass
		
	End Function
	
